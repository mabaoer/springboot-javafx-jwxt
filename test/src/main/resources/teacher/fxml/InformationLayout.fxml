<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="root" prefHeight="400.0" prefWidth="600.0" stylesheets="/teacher/css/InformationStyle.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="example.controller.teacher.InformationController">
    <children>
        <GridPane AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <GridPane>
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label fx:id="teacherName" alignment="CENTER" prefWidth="1900" text="Name:" GridPane.rowIndex="0" />
                        <Label fx:id="title" alignment="CENTER" prefWidth="1900" text="title:" GridPane.rowIndex="1" />
                        <Label fx:id="degree" alignment="CENTER" prefWidth="1900" text="degree:" GridPane.rowIndex="2" />
                        <Label fx:id="college" alignment="CENTER" prefWidth="1900" text="collage:" GridPane.rowIndex="3" />
                        <Label fx:id="grade" alignment="CENTER" prefWidth="1900" text="grade:" GridPane.rowIndex="4" />
                    </children>
                </GridPane>
                <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="1">
                    <columns>
                        <TableColumn fx:id="courseId" prefWidth="270.0" text="courseId" />
                        <TableColumn fx:id="courseName" prefWidth="270.0" text="courseName" />
                        <TableColumn fx:id="number" prefWidth="270.0" text="number" />
                        <TableColumn fx:id="courseType" prefWidth="270.0" text="courseType" />
                        <TableColumn fx:id="collegeId" prefWidth="270.0" text="collegeId" />
                        <TableColumn fx:id="trainProId" prefWidth="270.0" text="trainProId" />
                        <TableColumn fx:id="status" prefWidth="270.0" text="status" />
                    </columns>
                </TableView>
            </children>
        </GridPane>
    </children>
</AnchorPane>
