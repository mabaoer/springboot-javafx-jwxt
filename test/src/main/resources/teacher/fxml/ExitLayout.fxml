<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="example.controller.teacher.ExitController"
            stylesheets="@/teacher/css/ExitStyle.css"
            prefHeight="400.0" prefWidth="600.0">
    <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
          AnchorPane.topAnchor="0.0"/>
    <Label text="Are you sure to exit?" AnchorPane.leftAnchor="200" AnchorPane.topAnchor="100"/>
    <Button fx:id="confirm" mnemonicParsing="false" text="Confirm" AnchorPane.bottomAnchor="100" onAction="#Confirm"
            AnchorPane.leftAnchor="200"/>
    <Button fx:id="cancel" mnemonicParsing="false" text="Cancel" AnchorPane.bottomAnchor="100" onAction="#Cancel"
            AnchorPane.rightAnchor="200"/>
</AnchorPane>
