<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="/teacher/css/CourseStyle.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="example.controller.teacher.CourseController">
    <children>
        <GridPane AnchorPane.bottomAnchor="80.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="80.0" AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label alignment="CENTER" prefHeight="30.0" prefWidth="900.0" text="courseName" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                <Label alignment="CENTER" prefHeight="30.0" prefWidth="900.0" text="number" GridPane.columnIndex="0" GridPane.rowIndex="2" />
                <Label alignment="CENTER" prefHeight="30.0" prefWidth="900.0" text="courseTime" GridPane.columnIndex="0" GridPane.rowIndex="3" />
                <Label alignment="CENTER" prefHeight="30.0" prefWidth="900.0" text="classRoom" GridPane.columnIndex="0" GridPane.rowIndex="4" />
                <Label alignment="CENTER" prefHeight="30.0" prefWidth="900.0" text="courseWeek" GridPane.columnIndex="0" GridPane.rowIndex="5" />
                <Label alignment="CENTER" prefHeight="30.0" prefWidth="900.0" text="courseType" GridPane.columnIndex="0" GridPane.rowIndex="6" />
                <Label alignment="CENTER" prefHeight="30.0" prefWidth="900.0" text="collegeId" GridPane.columnIndex="0" GridPane.rowIndex="7" />
                <Label alignment="CENTER" prefHeight="30.0" prefWidth="900.0" text="credit" GridPane.columnIndex="0" GridPane.rowIndex="8" />
                <Label alignment="CENTER" prefHeight="30.0" prefWidth="900.0" text="trainProId" GridPane.columnIndex="0" GridPane.rowIndex="9" />
                <Label alignment="CENTER" prefHeight="30.0" prefWidth="900.0" text="endTime" GridPane.columnIndex="0" GridPane.rowIndex="10" />
                <TextField fx:id="courseName" prefHeight="50" promptText="length &lt; 15" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <TextField fx:id="number" prefHeight="50" promptText="value &gt; 10" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <TextField fx:id="courseTime" prefHeight="50" promptText="Monday-Sunday" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <TextField fx:id="classRoom" prefHeight="50" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                <TextField fx:id="courseWeek" prefHeight="50" promptText="value &gt; 3" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                <TextField fx:id="courseType" prefHeight="50" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                <TextField fx:id="collegeId" prefHeight="50" promptText="value &lt; 10" GridPane.columnIndex="1" GridPane.rowIndex="7" />
                <TextField fx:id="credit" prefHeight="50" promptText="value &lt; 7" GridPane.columnIndex="1" GridPane.rowIndex="8" />
                <TextField fx:id="trainProId" prefHeight="50" promptText="value &lt; 10" GridPane.columnIndex="1" GridPane.rowIndex="9" />
                <TextField fx:id="endTime" prefHeight="50" promptText="yyyy-MM-dd" GridPane.columnIndex="1" GridPane.rowIndex="10" />
            </children>
        </GridPane>
        <ButtonBar prefHeight="80" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="100.0">
            <buttons>
                <Button fx:id="cancel" mnemonicParsing="false" onAction="#Cancel" prefHeight="80.0" text="Cancel" />
                <Button fx:id="commit" mnemonicParsing="false" onAction="#Commit" prefHeight="80.0" text="Commit" />
            </buttons>
        </ButtonBar>
    </children>
</AnchorPane>
